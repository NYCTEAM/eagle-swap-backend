{
  "name": "eagle-swap-backend",
  "version": "1.0.1",
  "description": "Eagle Swap Backend - DEX backend service with token management, trading, liquidity, and farming features",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc || true",
    "build:prod": "tsc || true",
    "prestart": "node init-on-startup.js || true",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:init": "ts-node src/database/init.ts",
    "db:seed": "ts-node src/database/seed.ts",
    "db:migrate": "echo 'Migration script not implemented yet'",
    "check": "tsc --noEmit",
    "clean": "npx rimraf dist || rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "defi",
    "dex",
    "swap",
    "liquidity",
    "farming",
    "blockchain",
    "ethereum",
    "bsc",
    "polygon"
  ],
  "author": "Eagle Team",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.2",
    "better-sqlite3": "^12.4.1",
    "big.js": "^6.2.1",
    "bs58": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "rss-parser": "^3.13.0",
    "sqlite3": "^5.1.6",
    "typescript": "^5.3.3",
    "web3": "^4.3.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/big.js": "^6.2.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/eagle-swap-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/eagle-swap-backend/issues"
  },
  "homepage": "https://github.com/your-org/eagle-swap-backend#readme"
}
